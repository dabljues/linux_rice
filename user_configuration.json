{
    "audio": "pulseaudio",
    "bootloader": "grub-install",
    "dry-run": false,
    "gfx_driver": "All open-source (default)",
    "harddrives": [
        "/dev/sda"
    ],
    "hostname": "arch",
    "kernels": [
        "linux"
    ],
    "keyboard-layout": "pl",
    "mirror-region": "Poland",
