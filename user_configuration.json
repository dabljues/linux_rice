        "path": "/usr/lib/python3.10/site-packages/archinstall/profiles/xorg.py"
    },
    "script": "guided",
    "swap": true,
    "sys-encoding": "utf-8",
    "sys-language": "en_US",
    "timezone": "Europe/Warsaw",
}